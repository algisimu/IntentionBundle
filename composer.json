{
  "name": "algisimu/intention-bundle",
  "type": "symfony-bundle",
  "description": "Helper bundle to simplify CRUD operations.",
  "keywords": ["CRUD", "API", "intention", "command"],
  "license": "MIT",
  "authors": [
    {
      "name": "Algirdas Simutis",
      "email": "algisimu@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "symfony/http-foundation": "~2.4|~3.0.0",
    "symfony/polyfill-php70": "~1.0",
    "symfony/validator": "~2.5|~3.0.0",
    "symfony/options-resolver": "~2.5|~3.0.0",
    "doctrine/orm": "~2.5"
  },
  "require-dev": {
    "symfony/phpunit-bridge": "~2.8|~3.0",
    "phpunit/phpunit": "^4.8|^5.1"
  },
  "autoload": {
    "psr-4": {
      "Algisimu\\IntentionBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.8-dev"
    }
  }
}
